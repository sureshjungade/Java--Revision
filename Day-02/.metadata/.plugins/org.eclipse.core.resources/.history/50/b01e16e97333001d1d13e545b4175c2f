package com.masai.fourth;

public class B {

	Class Animal(){

		String name;

		Animal(){
			this.name = "Lion"; 
		}

		public Animal(String name){
			this.name = name;
		}
	}


	Animal a = new Animal("tiger");
	System.out.println(a.name); 


	Animal b = new Animal(); 
	System.out.println(b.name); 
}
